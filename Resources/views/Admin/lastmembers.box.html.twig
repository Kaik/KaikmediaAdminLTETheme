<div class="box box-danger">
    <div class="box-header with-border">
        <h3 class="box-title">{{ __('Latest members', 'kaikmediaadminltetheme') }}</h3>

        <div class="box-tools pull-right">
            <span class="label label-danger">{{ latestRegisteredUsers()|length }} {{ __('New Members', 'kaikmediaadminltetheme') }}</span>
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
            </button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i>
            </button>
        </div>
    </div>
    <!-- /.box-header -->
    <div class="box-body no-padding">
        <ul class="users-list clearfix">
            {% for user in latestRegisteredUsers() %}
            <li>
                <img src="dist/img/user1-128x128.jpg" alt="User Image">
                <a class="users-list-name" href="#"> {{ user.name }}</a>
                <span class="users-list-date">Today</span>
            </li>
            {% endfor %}
        </ul>
        <!-- /.users-list -->
    </div>
    <!-- /.box-body -->
    <div class="box-footer text-center">
        <a href="{{ path('zikulausersmodule_useradministration_list') }}" class="uppercase">{{ __('View All Users', 'kaikmediaadminltetheme') }}</a>
    </div>
    <!-- /.box-footer -->
</div>